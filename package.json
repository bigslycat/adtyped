{
  "name": "igogo",
  "version": "1.0.1",
  "description": "Fast Either and Maybe from Fantasy Land",
  "repository": "git@github.com:bigslycat/igogo.git",
  "author": "Pavel Tereschenko <me@stupidcat.wtf> (https://github.com/bigslycat)",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "README.md",
    "LICENSE.md",
    "lib",
    "esm",
    "src"
  ],
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "rollup -c",
    "build:esm": "rm -rf esm && BABEL_ENV=esm node scripts/build-esm.js && yarn flow-entry",
    "flow-entry": "cp lib/index.js.flow esm/index.mjs.flow",
    "lint": "eslint .",
    "flow": "flow",
    "test": "ava --timeout=30s --verbose",
    "test:nyc": "nyc ava --timeout=30s --verbose",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "preversion": "git checkout master && yarn lint && yarn flow",
    "postversion": "git push --follow-tags"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn flow"
    }
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "nyc": {
    "require": [
      "esm",
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": [
      "**/index.js",
      "**/*.config.js",
      "**/*.test.js",
      "lib"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "core-js": "^2.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "ava": "^1.0.1",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.0",
    "eslint": "^5.10.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-flowtype": "^3.2.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "esm": "^3.0.84",
    "flow-bin": "^0.89.0",
    "husky": "^1.2.0",
    "nyc": "^13.1.0",
    "prettier": "^1.15.3",
    "rollup": "^0.67.4",
    "rollup-plugin-babel": "^4.1.0",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-flow-entry": "^0.2.0"
  },
  "keywords": [
    "Maybe",
    "Either",
    "ADT",
    "Flow",
    "Fantasy Land"
  ]
}
