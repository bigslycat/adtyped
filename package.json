{
  "name": "igogo",
  "version": "1.7.2",
  "description": "Fast Either and Maybe from Fantasy Land with Flow and TypeScript support",
  "repository": "git@github.com:bigslycat/igogo.git",
  "author": "Pavel Tereschenko <me@stupidcat.wtf> (https://github.com/bigslycat)",
  "license": "MIT",
  "main": "lib/index.js",
  "files": [
    "README.md",
    "LICENSE.md",
    "lib",
    "esm",
    "src"
  ],
  "types": "src/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint .",
    "flow": "flow",
    "test": "ava --verbose",
    "test:nyc": "nyc ava --verbose",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn flow"
    }
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "nyc": {
    "require": [
      "esm",
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "exclude": [
      "**/index.js",
      "**/*.config.js",
      "**/*.test.js",
      "lib"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "core-js": "^3.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.4.4",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/commit-analyzer": "^6.2.0",
    "@semantic-release/git": "^7.0.12",
    "@semantic-release/github": "^5.4.0",
    "@semantic-release/npm": "^5.1.9",
    "@semantic-release/release-notes-generator": "^7.2.0",
    "ava": "2.1.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.4",
    "eslint": "6.0.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-flowtype": "^3.10.3",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.1.0",
    "esm": "3.2.25",
    "flow-bin": "0.101.0",
    "husky": "^2.4.1",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "rollup": "1.15.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-flow-entry": "0.3.1",
    "semantic-release": "^15.13.16",
    "sinon": "7.3.2",
    "typescript": "3.5.1"
  },
  "keywords": [
    "Maybe",
    "Either",
    "ADT",
    "Flow",
    "TypeScript",
    "TS",
    "FL",
    "Fantasy Land"
  ]
}
